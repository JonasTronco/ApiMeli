{
    "name": "Biblioteca APIS ML",
    "description": "Conjunto de herramientas con el fin de optimizar los tiempos de respuesta a nuestros usuarios",
    "version": "1.2",
    "background": {
        "scripts": ["JS/jquery-3.3.1.min.js", "JS/background.js","JS/bootstrap.min.js", "JS/vue.js"],
        "persistent": false

    },
    "permissions": [
        "notifications",
        "storage",
        "activeTab",
        "http://*/*", "https://*/*",
        "cookies",
        "https://.mercadolibre.com"


    ],
    "browser_action": {
        "default_title": "Optimicase Mercado Libre",
        "default_icon": "128.png",
        "default_popup": "api_app.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}